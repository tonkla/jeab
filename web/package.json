{
  "name": "jeab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run watch:css && react-scripts start",
    "build": "yarn run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css --env production"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.5.1",
    "autoprefixer": "^9.8.6",
    "axios": "^0.19.2",
    "cssnano": "^4.1.10",
    "easy-peasy": "^3.3.1",
    "firebase": "^7.17.1",
    "google-auth-library": "^6.0.6",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.6.0",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
