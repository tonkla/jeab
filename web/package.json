{
  "name": "jeab-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn run watch:css && react-scripts start",
    "build": "yarn run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css --env production"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.5.1",
    "autoprefixer": "^10.0.2",
    "axios": "^0.21.0",
    "cssnano": "^4.1.10",
    "easy-peasy": "^4.0.1",
    "firebase": "^8.0.2",
    "google-auth-library": "^6.0.6",
    "postcss-cli": "^8.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "tailwindcss": "^1.6.0",
    "typescript": "~4.0.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
